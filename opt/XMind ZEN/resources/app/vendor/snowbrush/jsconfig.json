{
  "compilerOptions": {
    "target": "es2015",
    "baseUrl": ".",
    "paths": {
      "sb_core/*": [
        "./js/core/*"
      ],
      "sb_models/*": [
        "./js/models/*"
      ],
      "sb_modules/*": [
        "./js/modules/*"
      ],
      "sb_structures/*": [
